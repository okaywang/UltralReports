                        }*@
                </td>
            }
            <td></td>
        </tr>
        <tr>
            <td>五值</td>
            @for (int i = 0; i < colsA.Length; i++)
            {
                var subKey = colsA[i];
                var key = "5" + subKey;
                var val = dict.ContainsKey(key) ? dict[key].Value : 0.00m;
                var info = dictKpiGroupInfo.ContainsKey(subKey) ? dictKpiGroupInfo[subKey] : null;
