                <td>
                    @*@if (info != null && info.Delta != decimal.MaxValue)
                        {
                            var v = (info.Max - val) * info.Delta + info.Addition;
                            @(v.ToString("0.00"))
                        }*@
                </td>
            }
            <td></td>
        </tr>
        <tr>
            <td colspan="22"></td>
        </tr>
    </tbody>

